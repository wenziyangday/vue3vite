{
  "name": "vue3vite",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "vincent",
    "email": "wenziyangday@gmail.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "eslint:fix": "eslint --ext .tsx,.ts,.vue src",
    "prettier:fix": "prettier --write './**/*.{js,ts,vue,css,less,scss,json}'",
    "stylelint:fix": "stylelint --aei *.css,*.less --fix src/",
    "husky:prepare": "npx husky install && npx husky add .husky/pre-commit \"pnpm lint-staged\" && npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "axios": "^0.26.1",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/eslint-parser": "^7.22.7",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-standard": "^8.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.25",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "*",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^0.29.8",
    "webpack": "^5.88.1"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,tsx}": [
      "pnpm prettier:fix",
      "pnpm stylelint:fix",
      "pnpm eslint:fix"
    ],
    "**/*.{css,less,html}": [
      "pnpm stylelint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
